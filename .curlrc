# 在 Windows10 上伪装成 CHROME
user-agent = "Mozilla/5.0 (compatible; MSIE 9.0; Windows NT 6.1; Trident/5.0)"
user-agent = "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/66.0.3355.0 Safari/537.36"

# 在超时前等待 30 秒。
connect-timeout = 30

# 遵循 HTTP 重定向
location

# 执行重定向时，自动将上一个URL设置为 REFERER。
referer = ";auto"

# 显示错误信息 
show-error

# 不显示进度条
silent
